<view class="explore">
  <view class="title">探索</view>
  <van-search bind:change="searchChange" bind:search="onSearch" background="#eef1f3" shape="round" value="{{ searchValue }}" placeholder="搜索Github仓库" />
  <view class="find">
    <view>发现</view>
    <van-cell-group custom-class="feature-info">
      <van-cell title="热门仓库" icon="newspaper-o" is-link value="" url="/pages/trending/trending" size="large" />
      <van-cell title="精选列表" icon="smile-o" is-link value="" url="/pages/hot/hot" size="large" />
    </van-cell-group>
  </view>
</view>